@prefix yawl: <http://bitflow.ai/ontology/yawl/v2#> .
@prefix yawl-exec: <http://bitflow.ai/ontology/yawl/execution/v1#> .
@prefix yawl-pattern: <http://bitflow.ai/ontology/yawl/patterns/v1#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .

# =============================================================================
# EXTENDED YAWL ONTOLOGY v2.1
# Complete support for all 43 W3C workflow patterns + beyond
# Self-executing through pure Turtle definitions
# =============================================================================

# =============================================================================
# SECTION 1: CORE EXECUTION SEMANTICS (Makes Turtle executable)
# =============================================================================

yawl-exec:ExecutionSemantics a rdfs:Class ;
    rdfs:label "Execution Semantics" ;
    rdfs:comment "Defines how a workflow element behaves at runtime" .

# Task execution modes
yawl-exec:executionMode a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl-exec:ExecutionMode ;
    rdfs:comment "How the task executes: sync, async, parallel, queued" .

yawl-exec:ExecutionMode a rdfs:Class ;
    rdfs:label "Execution Mode" .

yawl-exec:Synchronous a yawl-exec:ExecutionMode ;
    rdfs:label "Synchronous" ;
    rdfs:comment "Blocks until completion" .

yawl-exec:Asynchronous a yawl-exec:ExecutionMode ;
    rdfs:label "Asynchronous" ;
    rdfs:comment "Non-blocking, callback on completion" .

yawl-exec:Queued a yawl-exec:ExecutionMode ;
    rdfs:label "Queued" ;
    rdfs:comment "Task queued for later execution" .

yawl-exec:Parallel a yawl-exec:ExecutionMode ;
    rdfs:label "Parallel" ;
    rdfs:comment "Executes in parallel with others" .

# Runtime behavior extraction
yawl-exec:runtimeBehavior a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "Reference to behavior implementation (code, service, etc)" .

yawl-exec:timeoutPolicy a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl-exec:TimeoutPolicy ;
    rdfs:comment "How to handle task timeouts" .

yawl-exec:TimeoutPolicy a rdfs:Class .

yawl-exec:TimeoutSkip a yawl-exec:TimeoutPolicy ;
    rdfs:label "Skip" ;
    rdfs:comment "Skip task and continue" .

yawl-exec:TimeoutRetry a yawl-exec:TimeoutPolicy ;
    rdfs:label "Retry" ;
    rdfs:comment "Retry task with backoff" .

yawl-exec:TimeoutEscalate a yawl-exec:TimeoutPolicy ;
    rdfs:label "Escalate" ;
    rdfs:comment "Route to escalation handler" .

yawl-exec:TaskDuration a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:duration ;
    rdfs:comment "Expected execution duration (for optimization)" .

yawl-exec:MaxConcurrency a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:integer ;
    rdfs:comment "Maximum concurrent instances of this task" .

yawl-exec:RetryPolicy a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl-exec:RetryPolicyType ;
    rdfs:comment "Retry behavior on failure" .

yawl-exec:RetryPolicyType a rdfs:Class .

yawl-exec:RetryExponential a yawl-exec:RetryPolicyType ;
    rdfs:label "Exponential Backoff" .

yawl-exec:RetryLinear a yawl-exec:RetryPolicyType ;
    rdfs:label "Linear Backoff" .

yawl-exec:RetryImmediate a yawl-exec:RetryPolicyType ;
    rdfs:label "Immediate Retry" .

# =============================================================================
# SECTION 2: COMPLETE PATTERN DEFINITIONS (All 43 + Beyond)
# =============================================================================

# W3C Workflow Pattern Ontology
yawl-pattern:WorkflowPattern a rdfs:Class ;
    rdfs:label "Workflow Pattern" ;
    rdfs:comment "W3C standardized workflow pattern" .

yawl-pattern:patternId a rdf:Property ;
    rdfs:domain yawl-pattern:WorkflowPattern ;
    rdfs:range xsd:integer ;
    rdfs:comment "W3C pattern number (1-43)" .

yawl-pattern:patternName a rdf:Property ;
    rdfs:domain yawl-pattern:WorkflowPattern ;
    rdfs:range xsd:string ;
    rdfs:comment "Canonical pattern name from W3C" .

# =============================================================================
# PATTERNS 1-5: BASIC CONTROL FLOW
# =============================================================================

yawl-pattern:Sequence a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 1 ;
    yawl-pattern:patternName "Sequence" ;
    rdfs:comment "A -> B (task follows task)" ;
    rdfs:label "Sequence Pattern" .

yawl-pattern:ParallelSplit a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 2 ;
    yawl-pattern:patternName "Parallel Split" ;
    rdfs:comment "A -> B AND C (AND-split)" ;
    rdfs:label "Parallel Split Pattern" ;
    yawl:requiredSplitType yawl:AND .

yawl-pattern:Synchronization a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 3 ;
    yawl-pattern:patternName "Synchronization" ;
    rdfs:comment "B AND C -> D (AND-join)" ;
    rdfs:label "Synchronization Pattern" ;
    yawl:requiredJoinType yawl:AND .

yawl-pattern:ExclusiveChoice a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 4 ;
    yawl-pattern:patternName "Exclusive Choice" ;
    rdfs:comment "A -> B XOR C (XOR-split with predicate)" ;
    rdfs:label "Exclusive Choice Pattern" ;
    yawl:requiredSplitType yawl:XOR ;
    yawl:requiresFlowPredicate true .

yawl-pattern:SimpleMerge a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 5 ;
    yawl-pattern:patternName "Simple Merge" ;
    rdfs:comment "B XOR C -> D (XOR-join)" ;
    rdfs:label "Simple Merge Pattern" ;
    yawl:requiredJoinType yawl:XOR .

# =============================================================================
# PATTERNS 6-9: ADVANCED BRANCHING
# =============================================================================

yawl-pattern:MultiChoice a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 6 ;
    yawl-pattern:patternName "Multi-Choice" ;
    rdfs:comment "A -> one or more of {B, C, D} (OR-split)" ;
    rdfs:label "Multi-Choice Pattern" ;
    yawl:requiredSplitType yawl:OR ;
    yawl:allowMultipleBranches true .

yawl-pattern:SynchronizingMerge a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 7 ;
    yawl-pattern:patternName "Synchronizing Merge" ;
    rdfs:comment "B OR C (with sync) -> D (OR-join)" ;
    rdfs:label "Synchronizing Merge Pattern" ;
    yawl:requiredJoinType yawl:OR ;
    yawl:requiresSynchronization true .

yawl-pattern:MultipleMerge a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 8 ;
    yawl-pattern:patternName "Multiple Merge" ;
    rdfs:comment "B OR C -> D (without sync)" ;
    rdfs:label "Multiple Merge Pattern" ;
    yawl:requiredJoinType yawl:OR ;
    yawl:requiresSynchronization false .

yawl-pattern:Discriminator a yawl-pattern:WorkflowPattern ;
    yawl-pattern:patternId 9 ;
    yawl-pattern:patternName "Discriminator" ;
    rdfs:comment "First of many to complete (quorum-based)" ;
    rdfs:label "Discriminator Pattern" ;
    yawl:requiredJoinType yawl:Discriminator ;
    yawl:requiresQuorum true .

# =============================================================================
# SECTION 3: MISSING PATTERN PROPERTIES (Now fully supported)
# =============================================================================

# Quorum-based joins (Pattern 9 and advanced)
yawl:Discriminator a rdfs:Class ;
    rdfs:label "Discriminator Join Type" ;
    rdfs:comment "Join type for discriminator (first N)" .

yawl:QuorumJoin a rdfs:Class ;
    rdfs:label "Quorum Join Type" ;
    rdfs:comment "Join when N of M complete" .

yawl:discriminatorThreshold a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:integer ;
    rdfs:comment "How many completions trigger join (for discriminator/quorum)" .

yawl:joinCondition a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:string ;
    rdfs:comment "Predicate for conditional join (e.g., 'count > 3')" .

# Arbitrary cycles (Pattern 11)
yawl:BackwardFlow a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl:Task ;
    rdfs:comment "Allows arbitrary cycles in workflow" .

yawl:CycleDetectionMode a rdfs:Class ;
    rdfs:label "Cycle Detection Mode" .

yawl:DepthLimited a yawl:CycleDetectionMode ;
    rdfs:label "Depth Limited" ;
    rdfs:comment "Limit cycle iterations to N" .

yawl:CounterBased a yawl:CycleDetectionMode ;
    rdfs:label "Counter Based" ;
    rdfs:comment "Use counter property to limit" .

yawl:MaxIterations a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:integer ;
    rdfs:comment "Maximum iterations for cyclic flows" .

yawl:IterationCounter a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:integer ;
    rdfs:comment "Current iteration count" .

# Deferred choice (Pattern 16)
yawl:DeferredChoice a rdf:Property ;
    rdfs:domain yawl:Condition ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Runtime decision point (not at design time)" .

yawl:DecisionPoint a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "Reference to decision logic" .

# Interleaved parallel (Pattern 24)
yawl:InterleavingMode a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl:InterleavingType ;
    rdfs:comment "How parallel tasks interleave" .

yawl:InterleavingType a rdfs:Class ;
    rdfs:label "Interleaving Type" .

yawl:FullyInterleaved a yawl:InterleavingType ;
    rdfs:label "Fully Interleaved" ;
    rdfs:comment "Any execution order" .

yawl:PartiallyInterleaved a yawl:InterleavingType ;
    rdfs:label "Partially Interleaved" ;
    rdfs:comment "Constrained order" .

yawl:ThreadOrdering a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:string ;
    rdfs:comment "Ordering constraint for interleaved threads" .

# Milestone pattern (Pattern 27)
yawl:Milestone a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Task is a milestone (checkpoint)" .

yawl:MilestoneCondition a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:string ;
    rdfs:comment "Predicate for milestone completion" .

yawl:TimeoutMs a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:integer ;
    rdfs:comment "Timeout in milliseconds for milestone" .

# Critical section (Pattern 25)
yawl:CriticalSection a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl:Task ;
    rdfs:comment "Tasks that cannot execute concurrently" .

yawl:ExclusivityScope a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range xsd:string ;
    rdfs:comment "Scope of mutual exclusion" .

yawl:Semaphore a rdf:Property ;
    rdfs:domain yawl:Condition ;
    rdfs:range xsd:integer ;
    rdfs:comment "Semaphore capacity for critical section" .

# Cancellation patterns (19-21)
yawl:CancelScope a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl:CancelScopeType ;
    rdfs:comment "What gets cancelled when this task triggers cancel" .

yawl:CancelScopeType a rdfs:Class ;
    rdfs:label "Cancel Scope Type" .

yawl:CancelTask a yawl:CancelScopeType ;
    rdfs:label "Cancel Task" ;
    rdfs:comment "Cancel only this task" .

yawl:CancelCase a yawl:CancelScopeType ;
    rdfs:label "Cancel Case" ;
    rdfs:comment "Cancel entire workflow case" .

yawl:CancelRegion a yawl:CancelScopeType ;
    rdfs:label "Cancel Region" ;
    rdfs:comment "Cancel region of tasks" .

yawl:CancellationTarget a rdf:Property ;
    rdfs:domain yawl:Task ;
    rdfs:range yawl:Task ;
    rdfs:comment "Task(s) to cancel" .

# =============================================================================
# SECTION 4: PATTERN PERMUTATION RULES (Enables generative patterns)
# =============================================================================

yawl:requiredSplitType a rdf:Property ;
    rdfs:label "Required Split Type" ;
    rdfs:comment "Split type required for this pattern" .

yawl:requiredJoinType a rdf:Property ;
    rdfs:label "Required Join Type" ;
    rdfs:comment "Join type required for this pattern" .

yawl:requiresFlowPredicate a rdf:Property ;
    rdfs:label "Requires Flow Predicate" ;
    rdfs:comment "Pattern requires conditional flow" .

yawl:allowMultipleBranches a rdf:Property ;
    rdfs:label "Allow Multiple Branches" ;
    rdfs:comment "Can have 2+ branches" .

yawl:requiresSynchronization a rdf:Property ;
    rdfs:label "Requires Synchronization" ;
    rdfs:comment "Branches must synchronize" .

yawl:requiresQuorum a rdf:Property ;
    rdfs:label "Requires Quorum" ;
    rdfs:comment "Join based on quorum not all branches" .

# Valid split-join combinations (permutation matrix)
# This enables the system to verify any configuration is valid

yawl:SplitJoinCombination a rdfs:Class ;
    rdfs:label "Valid Split-Join Combination" ;
    rdfs:comment "Defines valid combinations of split and join types" .

yawl:splitType a rdf:Property ;
    rdfs:domain yawl:SplitJoinCombination ;
    rdfs:range yawl:SplitType .

yawl:joinType a rdf:Property ;
    rdfs:domain yawl:SplitJoinCombination ;
    rdfs:range yawl:JoinType .

yawl:isValid a rdf:Property ;
    rdfs:domain yawl:SplitJoinCombination ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Whether this combination is valid" .

yawl:generatesPattern a rdf:Property ;
    rdfs:domain yawl:SplitJoinCombination ;
    rdfs:range yawl-pattern:WorkflowPattern ;
    rdfs:comment "Which W3C pattern this combination implements" .

