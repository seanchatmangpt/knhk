@prefix yawl: <http://bitflow.ai/ontology/yawl/v2#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vdaalst: <http://bitflow.ai/ontology/vdaalst/patterns#> .

# ============================================================================
# VAN DER AALST 43 PATTERNS - ALL PATTERNS IN TURTLE FORMAT
# ============================================================================
# This file demonstrates how to represent all 43 Van der Aalst workflow
# patterns using Turtle RDF format with YAWL and Van der Aalst ontologies.
# ============================================================================

# ============================================================================
# BASIC CONTROL FLOW PATTERNS (1-5)
# ============================================================================

<http://example.org/workflow/pattern-1-sequence> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 1: Sequence" ;
    rdfs:comment "Execute multiple activities in sequential order" ;
    yawl:appliesPattern vdaalst:Pattern1Sequence ;
    yawl:hasTask <http://example.org/task/seq-task1>, <http://example.org/task/seq-task2>, <http://example.org/task/seq-task3> .

<http://example.org/task/seq-task1> a yawl:Task ;
    rdfs:label "Task 1" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasOutgoingFlow <http://example.org/condition/seq-cond-1-2> .

<http://example.org/task/seq-task2> a yawl:Task ;
    rdfs:label "Task 2" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/seq-cond-1-2> ;
    yawl:hasOutgoingFlow <http://example.org/condition/seq-cond-2-3> .

<http://example.org/task/seq-task3> a yawl:Task ;
    rdfs:label "Task 3" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/seq-cond-2-3> .

<http://example.org/condition/seq-cond-1-2> a yawl:Condition .
<http://example.org/condition/seq-cond-2-3> a yawl:Condition .

# ============================================================================
# PATTERN 2: PARALLEL SPLIT
# ============================================================================

<http://example.org/workflow/pattern-2-parallel-split> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 2: Parallel Split" ;
    rdfs:comment "Diverge into parallel branches" ;
    yawl:appliesPattern vdaalst:Pattern2ParallelSplit ;
    yawl:hasTask <http://example.org/task/split-start>, <http://example.org/task/split-branch1>, <http://example.org/task/split-branch2>, <http://example.org/task/split-branch3> .

<http://example.org/task/split-start> a yawl:Task ;
    rdfs:label "Start" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasOutgoingFlow <http://example.org/condition/split-1>, <http://example.org/condition/split-2>, <http://example.org/condition/split-3> .

<http://example.org/task/split-branch1> a yawl:Task ;
    rdfs:label "Branch 1" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/split-1> .

<http://example.org/task/split-branch2> a yawl:Task ;
    rdfs:label "Branch 2" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/split-2> .

<http://example.org/task/split-branch3> a yawl:Task ;
    rdfs:label "Branch 3" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/split-3> .

<http://example.org/condition/split-1> a yawl:Condition .
<http://example.org/condition/split-2> a yawl:Condition .
<http://example.org/condition/split-3> a yawl:Condition .

# ============================================================================
# PATTERN 3: SYNCHRONIZATION
# ============================================================================

<http://example.org/workflow/pattern-3-synchronization> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 3: Synchronization" ;
    rdfs:comment "Synchronize parallel branches" ;
    yawl:appliesPattern vdaalst:Pattern3Synchronization ;
    yawl:hasTask <http://example.org/task/sync-join> .

<http://example.org/task/sync-join> a yawl:Task ;
    rdfs:label "Synchronization Point" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/sync-from1>, <http://example.org/condition/sync-from2>, <http://example.org/condition/sync-from3> ;
    vdaalst:requiresAllIncoming true .

<http://example.org/condition/sync-from1> a yawl:Condition .
<http://example.org/condition/sync-from2> a yawl:Condition .
<http://example.org/condition/sync-from3> a yawl:Condition .

# ============================================================================
# PATTERN 4: EXCLUSIVE CHOICE
# ============================================================================

<http://example.org/workflow/pattern-4-exclusive-choice> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 4: Exclusive Choice" ;
    rdfs:comment "Choose one branch based on condition" ;
    yawl:appliesPattern vdaalst:Pattern4ExclusiveChoice ;
    yawl:hasTask <http://example.org/task/xor-choice>, <http://example.org/task/xor-option1>, <http://example.org/task/xor-option2> .

<http://example.org/task/xor-choice> a yawl:Task ;
    rdfs:label "Decision" ;
    yawl:hasSplitType yawl:XOR ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasOutgoingFlow <http://example.org/condition/xor-yes>, <http://example.org/condition/xor-no> .

<http://example.org/task/xor-option1> a yawl:Task ;
    rdfs:label "Option 1" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/xor-yes> .

<http://example.org/task/xor-option2> a yawl:Task ;
    rdfs:label "Option 2" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/xor-no> .

<http://example.org/condition/xor-yes> a yawl:Condition ;
    rdfs:label "Condition True" ;
    yawl:hasConditionExpression "[amount > 1000]" .

<http://example.org/condition/xor-no> a yawl:Condition ;
    rdfs:label "Condition False" ;
    yawl:hasConditionExpression "[amount <= 1000]" .

# ============================================================================
# PATTERN 5: SIMPLE MERGE
# ============================================================================

<http://example.org/workflow/pattern-5-simple-merge> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 5: Simple Merge" ;
    rdfs:comment "Merge exclusive branches" ;
    yawl:appliesPattern vdaalst:Pattern5SimpleMerge ;
    yawl:hasTask <http://example.org/task/merge-point> .

<http://example.org/task/merge-point> a yawl:Task ;
    rdfs:label "Merge Point" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:XOR ;
    yawl:hasIncomingFlow <http://example.org/condition/merge-from1>, <http://example.org/condition/merge-from2> ;
    vdaalst:assumesExclusiveIncoming true .

<http://example.org/condition/merge-from1> a yawl:Condition .
<http://example.org/condition/merge-from2> a yawl:Condition .

# ============================================================================
# PATTERN 6: MULTI-CHOICE
# ============================================================================

<http://example.org/workflow/pattern-6-multi-choice> a yawl:WorkflowSpecification ;
    rdfs:label "Pattern 6: Multi-Choice" ;
    rdfs:comment "Choose multiple branches" ;
    yawl:appliesPattern vdaalst:Pattern6MultiChoice ;
    yawl:hasTask <http://example.org/task/multi-choice>, <http://example.org/task/option-a>, <http://example.org/task/option-b>, <http://example.org/task/option-c> .

<http://example.org/task/multi-choice> a yawl:Task ;
    rdfs:label "Multi-Choice Decision" ;
    yawl:hasSplitType yawl:OR ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasOutgoingFlow <http://example.org/condition/mc-a>, <http://example.org/condition/mc-b>, <http://example.org/condition/mc-c> .

<http://example.org/task/option-a> a yawl:Task ;
    rdfs:label "Option A" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/mc-a> .

<http://example.org/task/option-b> a yawl:Task ;
    rdfs:label "Option B" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/mc-b> .

<http://example.org/task/option-c> a yawl:Task ;
    rdfs:label "Option C" ;
    yawl:hasSplitType yawl:AND ;
    yawl:hasJoinType yawl:AND ;
    yawl:hasIncomingFlow <http://example.org/condition/mc-c> .

<http://example.org/condition/mc-a> a yawl:Condition .
<http://example.org/condition/mc-b> a yawl:Condition .
<http://example.org/condition/mc-c> a yawl:Condition .

