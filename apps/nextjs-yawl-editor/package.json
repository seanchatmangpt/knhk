{
  "name": "nextjs-yawl-editor",
  "version": "0.1.0",
  "description": "Modern web-based YAWL workflow editor with RDF/Turtle support",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/components tests/lib --passWithNoTests",
    "test:integration": "jest tests/integration --passWithNoTests",
    "test:performance": "jest tests/performance --passWithNoTests",
    "test:chicago": "npm run test:performance",
    "test:weaver": "jest tests/weaver --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:weaver",
    "test:ci": "npm run type-check && npm run lint && npm run test:all && npm run test:e2e",
    "playwright:install": "playwright install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "^15.0.3",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5",
    "lucide-react": "^0.462.0",
    "zustand": "^5.0.2",
    "zod": "^3.23.8",
    "n3": "^1.22.4",
    "@rdfjs/types": "^1.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.2",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.2",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.54.2",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/n3": "^1.21.5",
    "typescript": "^5.7.2",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.0.3",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "@tailwindcss/typography": "^0.5.15",
    "tailwindcss": "^3.4.15",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@playwright/test": "^1.48.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "yawl",
    "workflow",
    "bpm",
    "rdf",
    "turtle",
    "ontology",
    "editor",
    "nextjs",
    "react",
    "opentelemetry",
    "doctrine"
  ],
  "author": "KNHK Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/knhk",
    "directory": "apps/nextjs-yawl-editor"
  }
}
